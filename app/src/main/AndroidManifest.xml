<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.engefour.gerirvendas">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Venda"
            android:label="@string/nova_venda" />
        <activity android:name=".NovoCliente"
            android:label="@string/novo_cliente"/>
        <activity android:name=".NovoProduto"
            android:label="@string/novo_produto"></activity>
    </application>
    <!--<uses-permission android:name="android.permission.CALL_PHONE"></uses-permission>-->

</manifest>